FROM soc_has_pin 
INNER JOIN pin_has_name ON soc_has_pin.pin_id = pin_has_name.pin_id
INNER JOIN pin_name ON pin_name.id = pin_has_name.pin_name_id
WHERE soc_has_pin.mcu_num != soc_has_pin.system_num AND pin_has_name.pin_mode_id = 0
ORDER BY soc_has_pin.soc_id,soc_has_pin.chip_num,soc_has_pin.chip_offset</sql><current_tab id="1"/></tab_sql></sqlb_project>
